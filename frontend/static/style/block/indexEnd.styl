.index__end{
  position: relative
  background-color white
  &:after{
    content ''
    position: absolute
    top: -15px
    left: 0px
    background url("/static/img/end23.png") no-repeat
    background-size cover
    background-position center
    width: 100%
    height: 15px
    z-index: 2
  }
  &_wrapper{
    position: relative
    height: 330px
    overflow: hidden
    &:after{
      content ''
      position: absolute
      top: 0px
      left: 0px
      background url("/static/img/end1.png") no-repeat
      background-size 100%
      width: 100%
      height: 517px
      z-index: 1
    }
  }
  &_title{
    text-align center
    position: relative
    z-index: 2
    font-size: 71px;
    color: #000000;
    margin-top: 65px
  }
}
@media (max-width: 1440px) {
  .index__end_wrapper:after{
    top: 50px
  }
}